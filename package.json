{
  "name": "note-app",
  "version": "1.0.0",
  "description": "An Electron application with Vue and TypeScript",
  "main": "./out/main/index.js",
  "author": "example.com",
  "homepage": "https://electron-vite.org",
  "scripts": {
    "format": "prettier --write .",
    "lint": "eslint --cache .",
    "typecheck:node": "tsc --noEmit -p tsconfig.node.json --composite false",
    "typecheck:web": "vue-tsc --noEmit -p tsconfig.web.json --composite false",
    "typecheck": "npm run typecheck:node && npm run typecheck:web",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:report": "playwright show-report",
    "test:e2e:electron": "playwright test --project=electron",
    "test:all": "npm run test:run && npm run test:e2e",
    "start": "electron-vite preview",
    "dev": "electron-vite dev",
    "build": "npm run typecheck && electron-vite build",
    "postinstall": "electron-builder install-app-deps",
    "build:unpack": "npm run build && electron-builder --dir",
    "build:win": "npm run build && electron-builder --win",
    "build:mac": "npm run build && electron-builder --mac",
    "build:linux": "npm run build && electron-builder --linux"
  },
  "dependencies": {
    "@codemirror/lang-markdown": "^6.3.4",
    "@codemirror/state": "^6.5.2",
    "@codemirror/theme-one-dark": "^6.1.3",
    "@codemirror/view": "^6.38.1",
    "@electron-toolkit/preload": "^3.0.2",
    "@electron-toolkit/utils": "^4.0.0",
    "@headlessui/vue": "^1.7.23",
    "@heroicons/vue": "^2.2.0",
    "@lexical/clipboard": "^0.34.0",
    "@lexical/code": "^0.34.0",
    "@lexical/code-shiki": "^0.34.0",
    "@lexical/file": "^0.34.0",
    "@lexical/history": "^0.34.0",
    "@lexical/html": "^0.34.0",
    "@lexical/link": "^0.34.0",
    "@lexical/list": "^0.34.0",
    "@lexical/markdown": "^0.34.0",
    "@lexical/rich-text": "^0.34.0",
    "@lexical/selection": "^0.34.0",
    "@lexical/table": "^0.34.0",
    "@lexical/utils": "^0.34.0",
    "@prettier/plugin-php": "^0.24.0",
    "chart.js": "^4.5.0",
    "chokidar": "^4.0.3",
    "electron-store": "^10.1.0",
    "electron-updater": "^6.3.9",
    "fuse.js": "^7.1.0",
    "highlight.js": "^11.11.1",
    "isolated-vm": "^6.0.1",
    "katex": "^0.16.22",
    "lexical": "^0.34.0",
    "markdown-it": "^14.1.0",
    "markdown-it-footnote": "^4.0.0",
    "markdown-it-highlightjs": "^4.2.0",
    "markdown-it-katex": "^2.0.3",
    "markdown-it-table": "^4.1.1",
    "markdown-it-task-lists": "^2.1.1",
    "pinia": "^3.0.3",
    "prismjs": "^1.30.0",
    "unocss": "^66.4.1",
    "vue-chartjs": "^5.3.2"
  },
  "devDependencies": {
    "@electron-toolkit/eslint-config-prettier": "3.0.0",
    "@electron-toolkit/eslint-config-ts": "^3.0.0",
    "@electron-toolkit/tsconfig": "^1.0.1",
    "@playwright/test": "^1.54.2",
    "@types/markdown-it": "^14.1.2",
    "@types/node": "^22.16.5",
    "@unocss/preset-attributify": "^66.4.1",
    "@unocss/preset-icons": "^66.4.1",
    "@unocss/preset-uno": "^66.4.1",
    "@vitejs/plugin-vue": "^6.0.0",
    "@vitest/ui": "^3.2.4",
    "@vue/test-utils": "^2.4.6",
    "electron": "^37.2.3",
    "electron-builder": "^25.1.8",
    "electron-playwright-helpers": "^1.7.1",
    "electron-vite": "^4.0.0",
    "eslint": "^9.31.0",
    "eslint-plugin-vue": "^10.3.0",
    "jsdom": "^26.1.0",
    "prettier": "^3.6.2",
    "typescript": "^5.8.3",
    "vite": "^7.0.5",
    "vitest": "^3.2.4",
    "vue": "^3.5.17",
    "vue-eslint-parser": "^10.2.0",
    "vue-tsc": "^3.0.3"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "electron",
      "esbuild"
    ]
  }
}
