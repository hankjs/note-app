{
  "name": "note-app",
  "version": "1.0.0",
  "description": "An Electron application with Vue and TypeScript",
  "main": "./out/main/index.js",
  "author": "example.com",
  "homepage": "https://electron-vite.org",
  "scripts": {
    "format": "prettier --write .",
    "lint": "eslint --cache .",
    "typecheck:node": "tsc --noEmit -p tsconfig.node.json --composite false",
    "typecheck:web": "vue-tsc --noEmit -p tsconfig.web.json --composite false",
    "typecheck": "npm run typecheck:node && npm run typecheck:web",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:report": "playwright show-report",
    "test:e2e:electron": "playwright test --project=electron",
    "test:all": "npm run test:run && npm run test:e2e",
    "start": "electron-vite dev",
    "dev": "electron-vite dev",
    "preview": "electron-vite preview",
    "build": "npm run typecheck && electron-vite build",
    "postinstall": "electron-builder install-app-deps",
    "build:unpack": "npm run build && electron-builder --dir",
    "build:win": "npm run build && electron-builder --win",
    "build:mac": "npm run build && electron-builder --mac",
    "build:linux": "npm run build && electron-builder --linux"
  },
  "dependencies": {
    "@codemirror/lang-markdown": "^6.3.4",
    "@codemirror/state": "^6.5.2",
    "@codemirror/theme-one-dark": "^6.1.3",
    "@codemirror/view": "^6.38.1",
    "@electron-toolkit/preload": "^3.0.2",
    "@electron-toolkit/utils": "^4.0.0",
    "@excalidraw/excalidraw": "^0.18.0",
    "@floating-ui/vue": "^1.1.9",
    "@headlessui/vue": "^1.7.23",
    "@heroicons/vue": "^2.2.0",
    "@lexical/clipboard": "catalog:",
    "@lexical/code": "catalog:",
    "@lexical/code-shiki": "catalog:",
    "@lexical/dragon": "catalog:",
    "@lexical/file": "catalog:",
    "@lexical/history": "catalog:",
    "@lexical/html": "catalog:",
    "@lexical/link": "catalog:",
    "@lexical/list": "catalog:",
    "@lexical/markdown": "catalog:",
    "@lexical/rich-text": "catalog:",
    "@lexical/selection": "catalog:",
    "@lexical/table": "catalog:",
    "@lexical/utils": "catalog:",
    "@prettier/plugin-php": "^0.24.0",
    "@vitejs/plugin-react": "^5.0.4",
    "@vitejs/plugin-vue-jsx": "^5.1.1",
    "@vueuse/core": "^13.9.0",
    "chart.js": "^4.5.0",
    "chokidar": "^4.0.3",
    "electron-store": "^10.1.0",
    "electron-updater": "^6.3.9",
    "fuse.js": "^7.1.0",
    "highlight.js": "^11.11.1",
    "inline-style-parser": "^0.2.4",
    "isolated-vm": "^6.0.1",
    "katex": "^0.16.22",
    "lexical": "catalog:",
    "lexical-vue": "workspace:*",
    "markdown-it": "^14.1.0",
    "markdown-it-footnote": "^4.0.0",
    "markdown-it-highlightjs": "^4.2.0",
    "markdown-it-katex": "^2.0.3",
    "markdown-it-table": "^4.1.1",
    "markdown-it-task-lists": "^2.1.1",
    "pinia": "^3.0.3",
    "unocss": "^66.4.1",
    "veaury": "^2.6.3",
    "vue": "catalog:",
    "vue-chartjs": "^5.3.2",
    "prismjs": "catalog:",
    "y-websocket": "^3.0.0"
  },
  "devDependencies": {
    "@electron-toolkit/eslint-config-prettier": "3.0.0",
    "@electron-toolkit/eslint-config-ts": "^3.0.0",
    "@electron-toolkit/tsconfig": "^1.0.1",
    "@playwright/test": "^1.54.2",
    "@types/markdown-it": "^14.1.2",
    "@types/node": "^22.16.5",
    "@types/prismjs": "catalog:",
    "@unocss/preset-attributify": "^66.4.1",
    "@unocss/preset-icons": "^66.4.1",
    "@unocss/preset-uno": "^66.4.1",
    "@vitejs/plugin-vue": "^6.0.0",
    "@vitest/ui": "^3.2.4",
    "@vue/test-utils": "^2.4.6",
    "electron": "^37.2.3",
    "electron-builder": "^25.1.8",
    "electron-playwright-helpers": "^1.7.1",
    "electron-vite": "^4.0.0",
    "eslint": "^9.31.0",
    "eslint-plugin-vue": "^10.3.0",
    "jsdom": "^26.1.0",
    "prettier": "^3.6.2",
    "typescript": "^5.8.3",
    "vite": "^7.0.5",
    "vitest": "^3.2.4",
    "vue-component-type-helpers": "^3.1.0",
    "vue-eslint-parser": "^10.2.0",
    "vue-tsc": "^3.0.3"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "electron",
      "esbuild",
      "isolated-vm"
    ]
  },
  "packageManager": "pnpm@9.15.0",
  "engines": {
    "node": "22.16.0"
  },
  "volta": {
    "node": "22.16.0"
  }
}
